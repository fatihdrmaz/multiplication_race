{
  "expo": {
    "name": "Vroom - Çarpım Yarışı",
    "slug": "vroom-carpim-oyunu",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#4FC3F7",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vroom.carpimoyunu"
    },
    "android": {
      "package": "com.vroom.carpimoyunu",
      "backgroundColor": "#4FC3F7"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ]
    ]
  }
}

